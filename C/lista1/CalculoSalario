/* Faça um programa que pergunte o salário por hora e o número de horas trabalhadas no mês. Calcule e mostre o total do salário no referido mês. */

#include <stdio.h>

int main()
{
float sH, tH;

printf("Digite o salário por hora e o número de horas trabalhadas no mês\n");
scanf("%f" "%f", &sH, &tH);


printf("O salario mensal é: %2.f",sH*tH);
	return 0;
}
